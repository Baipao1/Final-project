#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_node-xlsx@0.21.2@node-xlsx/lib/bin/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../_node-xlsx@0.21.2@node-xlsx/lib/bin/cli.js" "$@"
  ret=$?
fi
exit $ret
